{
    "extends": "airbnb",
    "plugins": [
        "import"
    ],
    "rules": {
      "indent": [2, "tab", { "SwitchCase": 1, "VariableDeclarator": 1 }],
      "no-tabs": 0,
      "arrow-parens": 0,
      "no-use-before-define": 0,
      "consistent-return": 0,
      "no-throw-literal": 0,
      "guard-for-in": 0,
      "no-restricted-syntax": 0
    }
}
